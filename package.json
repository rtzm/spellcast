{
  "name": "spellcast",
  "version": "0.1.0",
  "description": "Computer vision app to turn cellphone camera video into handwriting, and your phone into a wand Edit",
  "dependencies": {
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "chai": "^4.0.1",
    "express": "^4.15.5",
    "hbs": "~4.0.1",
    "morgan": "~1.9.0",
    "serve-favicon": "~2.4.5",
    "draw-from-video": "^0.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "mocha": "^3.4.2",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "scripts": {
    "serve": "webpack-dev-server",
    "test": "mocha --require babel-polyfill --require babel-register",
    "build:wasm": "wasm-pack build --out-dir ../../dist/draw-from-video --target web ./wasm/draw-from-video",
    "bundle:wasm": "wasm-pack build --target bundler ./wasm/draw-from-video",
    "link:wasm": "npm link ./wasm/draw-from-video/pkg"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rtzm/spellcast.git"
  },
  "keywords": [
    "computer",
    "vision",
    "video"
  ],
  "author": "rtzm",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rtzm/spellcast/issues"
  },
  "homepage": "https://github.com/rtzm/spellcast#readme"
}
